{
  "extends": [
    "react-app",
    "react-app/jest"
  ],
  "rules": {
    "no-unused-vars": "warn",
    "react-hooks/exhaustive-deps": "warn",
    "import/no-anonymous-default-export": "warn"
  },
  "overrides": [
    {
      "files": ["**/*.js", "**/*.jsx"],
      "rules": {
        "no-unused-vars": "off",
        "react-hooks/exhaustive-deps": "off",
        "import/no-anonymous-default-export": "off"
      }
    }
  ]
}